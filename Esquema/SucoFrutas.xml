    <CalculatedMember name="Variacao da Meta Custo" formatString="" caption="Varia&#231;&#227;o da Meta Custo" formula="((([Measures].[Custo Fixo]+[Measures].[Custo Variavel])/[Measures].[Meta Custo])-1)*100" dimension="Measures" visible="true">
    </CalculatedMember>
    <CalculatedMember name="Variacao da Meta Margem" formatString="" caption="Varia&#231;&#227;o da Meta Margem" formula="((([Measures].[Margem])/([Measures].[Meta Faturamento]-[Measures].[Meta Custo]))-1)*100" dimension="Measures" visible="true">
